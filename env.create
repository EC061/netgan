# TF2.0
conda create --name netgan python=3.7 tensorflow-gpu=2.6.0 cudatoolkit=11.1.1 -c conda-forge
# TF1.0
conda create --name netgantf1 python=3.6 tensorflow=1.4.0 -c conda-forge

conda activate netgan
conda install -c anaconda scikit-learn  matplotlib
